<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="ncmdumpGUI_OptionsDialog">
		<title>Options</title>
		<centered>1</centered>
		<minsize>480,320</minsize>
		<extra_code>SetIcon(wxIcon(wxT(&quot;options__icon&quot;)));</extra_code>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<style>wxCAPTION|wxCLOSE_BOX|wxMINIMIZE_BOX|wxCLIP_CHILDREN</style>
		<handler function="OnClose" entry="EVT_CLOSE" />
		<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
			<pos>280,392</pos>
			<object class="wxBoxSizer" variable="BoxSizer1" member="no">
				<orient>wxVERTICAL</orient>
				<object class="sizeritem">
					<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
						<label>Files</label>
						<orient>wxVERTICAL</orient>
						<object class="sizeritem">
							<object class="wxCheckBox" name="ID_CHECKBOX1" variable="chkDelNCM" member="yes">
								<label>Delete .ncm file after converted successfully (be careful)</label>
							</object>
							<flag>wxALL</flag>
							<border>5</border>
							<option>1</option>
						</object>
					</object>
					<flag>wxALL|wxEXPAND</flag>
					<border>5</border>
					<option>1</option>
				</object>
				<object class="sizeritem">
					<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
						<label>Meta Data</label>
						<orient>wxVERTICAL</orient>
						<object class="sizeritem">
							<object class="wxCheckBox" name="ID_CHECKBOX2" variable="chkFixMeta" member="yes">
								<label>Fix Meta data after decrypted (unstable)</label>
								<checked>1</checked>
							</object>
							<flag>wxALL</flag>
							<border>5</border>
							<option>1</option>
						</object>
						<object class="sizeritem">
							<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
								<label>Fix Metadata seems not pretty stable. So if you don&apos;t care about the meta data (author, album, cover, etc...), don&apos;t check the box.</label>
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>5</border>
							<option>1</option>
						</object>
					</object>
					<flag>wxALL|wxEXPAND</flag>
					<border>5</border>
					<option>2</option>
				</object>
			</object>
		</object>
	</object>
</wxsmith>
